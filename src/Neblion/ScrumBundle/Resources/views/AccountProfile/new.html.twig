
{% extends 'NeblionScrumBundle:Layout:base.html.twig' %}

{% form_theme form 'NeblionScrumBundle:Form:fields.html.twig' %}

{% block body %}
{% include 'NeblionScrumBundle:Layout:breadcrumb.html.twig' with {'pathes': pathes} %}

<div class="page-header">
    <h1>{{ 'Profile creation:'|trans }}</h1>
</div>

<div class="row-fluid">
    <div class="span3">
    </div><!--/span-->
    
    
    <div class="span9">
        <form action="{{ path('profile_create') }}" method="post" {{ form_enctype(form) }} class="form-horizontal" novalidate>
            {{ form_widget(form) }}
            <div class="form-actions">
                <button type="submit" class="btn btn-primary">{{ 'Create'|trans }}</button>
            </div>
        </form>
    </div><!--/span-->
</div><!--/row-->

{% endblock body %}


